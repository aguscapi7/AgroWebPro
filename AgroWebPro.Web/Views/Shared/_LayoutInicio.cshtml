    <script>
        function AbrirEnlace(enlace) {
            window.location.href = enlace;
        }

        function OlvidoContrasenna() {
            swal.close();
            $("#modalPageLoad").modal("show");
            var correo = $("#txtCorreo").val();
            if (correo != '' && correo != undefined) {
                var parametros = {
                    correo: correo
                };
                $.ajax({
                    dataType: "json",
                    type: "POST",
                    url: '@Url.Action("OlvidoContrasenna", "Home")',
                    data: parametros,
                    success: function (data) {
                        $("#modalPageLoad").modal("hide");
                        debugger;
                        if (data.resultado == '@AgroWebPro.Utilitarios.Constantes.EstadoCorrecto')
                        {
                            swal({
                                title: 'Correcto',
                                text: data.mensaje,
                                type: 'success',
                                confirmButtonColor: '#3085d6',
                                confirmButtonText: 'Aceptar'
                            }).then(function (isConfirm) {

                            });
                        }
                        else
                        {
                            swal({
                                title: 'Error',
                                text: data.mensaje,
                                type: 'error',
                                confirmButtonColor: '#3085d6',
                                confirmButtonText: 'Aceptar'
                            }).then(function (isConfirm) {

                            });
                        }

                    }
                });
            }
            else {
                swal({
                    title: 'Correcto',
                    text: 'Debe ingresar el correo',
                    type: 'error',
                    confirmButtonColor: '#3085d6',
                    confirmButtonText: 'Aceptar'
                }).then(function (isConfirm) {

                });
            }
            return false;

        }

        function MostrarLogin() {
            CerrarMenu();
            swal({
                title: '',
                background: '#fff',
                showCloseButton: true,
                html:
                '<br/><br/><div class="row">' +
                    '<div class="col-md-12">' +
                        '<form action="@Url.Action("Login", "Home")" data-toggle="validator" id="formInicio" method="post" role="form"><div class="form-group">' +
                                                    '<label class="sr-only" for="exampleInputEmail2">Correo electrónico</label>' +
                '<input type="email" class="form-control" id="txtCorreo" placeholder="Correo electrónico" required name="correo">' +
                '</div>' +
                '<div class="form-group">' +
                '    <label class="sr-only" for="exampleInputPassword2">Contraseña</label>' +
                '    <input type="password" class="form-control" id="txtPassword" name="password" placeholder="Contraseña" required>' +
                '    <div class="bottom text-center"><a href="" onclick="return OlvidoContrasenna()" style="font-size:10px;">Olvidó su contraseña?</a></div>' +
                '</div>' +
                '<div class="form-group">' +
                '    <button class="btn btn-skin btn-block" type="submit">Ingresar</button>' +
                '</div>' +
                '</form>' +
                                        '</div>' +
                '<div class="bottom text-center">' +
                '    Nuevo aquí? <a href="#" id="btnRegistro" onclick="Registro()"><b>Regístrese</b></a>' +
                '</div>' +
                                    '</div>',
                confirmButtonColor: '#3085d6',
                confirmButtonText: 'Aceptar',

                showConfirmButton: false
            }).then(function (isConfirm) {

            });
        }

        function Registro() {
            AbrirEnlace("@Url.Action("Registro", "Home")");
        }

        function AbrirMenu() {
            if ($(".navbar-main-collapse").hasClass("collapse")) {
                $(".navbar-main-collapse").removeClass("collapse");
            }
            else {
                $(".navbar-main-collapse").addClass("collapse");
            }
        }

        function CerrarMenu() {
            $(".navbar-main-collapse").addClass("collapse");
        }
    </script>
</body>

