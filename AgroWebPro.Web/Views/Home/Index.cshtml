
@{
    ViewBag.Title = "Home Page";
    Layout = "~/Views/Shared/_LayoutInicio.cshtml";
}


<script src="~/Scripts/jquery-1.10.2.js"></script>
<script src="~/Scripts/bootstrap-table.js"></script>
<link href="~/Content/sweetalert.css" rel="stylesheet" />
<script src="~/Scripts/sweetalert.js"></script>
@if (Session["errorCredenciales"] != null)
{
    Session["errorCredenciales"] = null;
        <script>
            swal({
                title: 'Error',
                text: "Las credenciales no son válidas",
                type: 'error',
                confirmButtonColor: '#3085d6',
                confirmButtonText: 'Aceptar'
            });</script>
}
<!-- Section: intro -->
<section id="intro" class="intro">
    <div class="intro-content">
        <div class="container">
            <div class="row">
                <div class="col-lg-6">
                    <div class="wow fadeInDown" data-wow-offset="0" data-wow-delay="0.1s">
                        <h2 class="h-ultra">AgroWebPro</h2>
                    </div>
                    <div class="wow fadeInUp" data-wow-offset="0" data-wow-delay="0.1s">
                        <h4 class="h-light">Software online para la administración agropecuaria</h4>
                    </div>
                    <div class="well well-trans">
                        <div class="wow fadeInRight" data-wow-delay="0.1s">

                            <ul class="lead-list">
                                <li><span class="fa fa-check fa-2x icon-success"></span> <span class="list"><strong>Has crecer tu empresa</strong><br />Empieza tu mes gratis</span></li>
                            </ul>
                            <p class="text-right wow bounceIn" data-wow-delay="0.4s">
                                <a href="#" class="btn btn-skin btn-lg" onclick="AbrirEnlace('@Url.Action("Registro","Home")')">Registrarse <i class="fa fa-angle-right"></i></a>
                            </p>
                        </div>
                    </div>


                </div>
                <div class="col-lg-6">
                    <div class="wow fadeInUp" data-wow-duration="2s" data-wow-delay="0.2s">
                        @*<img src="~/img/dummy/img-1.png" class="img-responsive" alt="" />*@
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<section id="boxes" class="home-section paddingtop-80">

    <div class="container">
        <div class="row">
            <div class="col-sm-4 col-md-4">
                <div class="wow fadeInUp" data-wow-delay="0.2s">
                    <div class="box text-center">

                        <i class="fa fa-check fa-3x circled bg-skin"></i>
                        <h4 class="h-bold">Intuitivo</h4>
                        <p>
                            Fácil de usar. ¡Registrate y empezá a gestionar tu campo ya!.
                        </p>
                    </div>
                </div>
            </div>
            <div class="col-sm-4 col-md-4">
                <div class="wow fadeInUp" data-wow-delay="0.2s">
                    <div class="box text-center">

                        <i class="fa fa-list-alt fa-3x circled bg-skin"></i>
                        <h4 class="h-bold">Lleve el control</h4>
                        <p>
                            AgroWebPro le permite llevar un registro detallado de lo que pasa en su empresa a diario.
                        </p>
                    </div>
                </div>
            </div>
            <div class="col-sm-4 col-md-4">
                <div class="wow fadeInUp" data-wow-delay="0.2s">
                    <div class="box text-center">

                        <i class="fa fa-arrow-up fa-3x circled bg-skin"></i>
                        <h4 class="h-bold">Empiece a crecer</h4>
                        <p>
                            Cuenta con una serie de herramientas administrativas para hacer crecer su negocio.
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>

</section>
<section id="service" class="home-section nopadding paddingtop-60" style="margin-bottom:30px;">

    <div class="container">

        <div class="row">
            <div class="col-sm-6 col-md-6">
                <div class="wow fadeInUp" data-wow-delay="0.2s">
                    <img src="~/img/dummy/img-1.png" class="img-responsive" alt="" />
                </div>
            </div>
            <div class="col-sm-3 col-md-3">

                <div class="wow fadeInRight" data-wow-delay="0.1s">
                    <div class="service-box">
                        <div class="service-icon">
                            <span class="fa fa-map-marker fa-3x"></span>
                        </div>
                        <div class="service-desc">
                            <h5 class="h-light">Administración de terrenos</h5>
                            <p>Dibujá tus lotes y visualizalos en un solo lugar.</p>
                        </div>
                    </div>
                </div>

                <div class="wow fadeInRight" data-wow-delay="0.2s">
                    <div class="service-box">
                        <div class="service-icon">
                            <span class="fa fa-users fa-3x"></span>
                        </div>
                        <div class="service-desc">
                            <h5 class="h-light">Seguimiento de usuarios</h5>
                            <p>Visualiza los avances de tareas de los usuarios.</p>
                        </div>
                    </div>
                </div>
                <div class="wow fadeInRight" data-wow-delay="0.3s">
                    <div class="service-box">
                        <div class="service-icon">
                            <span class="fa fa-pie-chart fa-3x"></span>
                        </div>
                        <div class="service-desc">
                            <h5 class="h-light">Reportes</h5>
                            <p>Genera informes personalizados de los cultivos</p>
                        </div>
                    </div>
                </div>


            </div>
            <div class="col-sm-3 col-md-3">

                <div class="wow fadeInRight" data-wow-delay="0.1s">
                    <div class="service-box">
                        <div class="service-icon">
                            <span class="fa fa-list fa-3x"></span>
                        </div>
                        <div class="service-desc">
                            <h5 class="h-light">Asignación de tareas</h5>
                            <p>Gestioná tus tareas, para saber lo que está pasando en tu campo.</p>
                        </div>
                    </div>
                </div>

                <div class="wow fadeInRight" data-wow-delay="0.2s">
                    <div class="service-box">
                        <div class="service-icon">
                            <span class="fa fa-check fa-3x"></span>
                        </div>
                        <div class="service-desc">
                            <h5 class="h-light">Control de inventarios</h5>
                            <p>Tus movimientos de stock se generarán automáticamente.</p>
                        </div>
                    </div>
                </div>
                <div class="wow fadeInRight" data-wow-delay="0.3s">
                    <div class="service-box">
                        <div class="service-icon">
                            <span class="fa fa-dollar fa-3x"></span>
                        </div>
                        <div class="service-desc">
                            <h5 class="h-light">Control de ventas</h5>
                            <p>Genera informes personalizados</p>
                        </div>
                    </div>
                </div>

            </div>

        </div>
    </div>
</section>
<!-- /Section: services -->
<!-- /Section: services -->

