<!-- Section: pricing -->
<section id="pricing" class="home-section bg-gray paddingbot-60" >
    <div class="container marginbot-50">
        <div class="row">
            <div class="col-lg-8 col-lg-offset-2">
                <div class="wow lightSpeedIn" data-wow-delay="0.1s">
                    <div class="section-heading text-center">
                        <h2 class="h-bold">Tarifas y planes </h2>
                    </div>
                </div>
                <div class="divider-short"></div>
            </div>
        </div>
    </div>

    <div class="container">

        <div class="row">

            <div class="col-sm-3 pricing-box">
                <div class="wow bounceInUp" data-wow-delay="0.1s">
                    <div class="pricing-content general">
                        <h2>Subscripción gratis</h2>
                        <h3>$0<sup>.00</sup> <span></span></h3>
                        <ul>
                            <li>Acceso gratis por un mes</li>
                        </ul>
                        
                        <div class="price-bottom">
                            <a href="#" class="btn btn-skin btn-lg">Empezar</a>
                        </div>
                    </div>
                </div>
            </div>

            <div class="col-sm-3 pricing-box">
                <div class="wow bounceInUp" data-wow-delay="0.2s">
                    <div class="pricing-content general last">
                        <h2>Plan mensual</h2>
                        <h3>$5<sup>.00</sup> <span>/ por mes</span></h3>
                        <ul>
                            <li>Acceso ilimitado <i class="fa fa-check icon-success"></i></li>
                        </ul>

                        <div class="price-bottom">
                            <a href="#" class="btn btn-skin btn-lg">Empezar</a>
                        </div>
                    </div>
                </div>
            </div>

            <div class="col-sm-3 pricing-box featured-price">
                <div class="wow bounceInUp" data-wow-delay="0.3s">
                    <div class="pricing-content general last">
                        <h2>Plan semestral</h2>
                        <h3>$25<sup>.00</sup> <span>/ por semestre</span></h3>
                        <ul>
                            <li>Acceso ilimitado<i class="fa fa-check icon-success"></i></li>
                        </ul>

                        <div class="price-bottom">
                            <a href="#" class="btn btn-skin btn-lg">Empezar</a>
                        </div>
                    </div>
                </div>
            </div>

            <div class="col-sm-3 pricing-box featured-price">
                <div class="wow bounceInUp" data-wow-delay="0.3s">
                    <div class="pricing-content featured">
                        <h2>Plan anual</h2>
                        <h3>$40<sup>.00</sup> <span>/ por año</span></h3>
                        <ul>
                            <li>Acceso ilimitado <i class="fa fa-check icon-success"></i></li>
                        </ul>

                        <div class="price-bottom">
                            <a href="#" class="btn btn-skin btn-lg">Empezar</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
</section>
<!-- /Section: pricing -->

